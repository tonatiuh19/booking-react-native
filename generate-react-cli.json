{
  "usesTypeScript": true,
  "usesCssModule": true,
  "cssPreprocessor": "css",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "customTemplates": {
        "component": "components/templates/component/TemplateName.tsx",
        "style": "components/templates/component/TemplateName.style.tsx"
      },
      "path": "components",
      "withStyle": true,
      "withTest": false,
      "withStory": false,
      "withLazy": false
    },
    "page": {
      "customTemplates": {
        "component": "components/templates/component/TemplateName.tsx",
        "style": "components/templates/component/TemplateName.style.tsx"
      },
      "path": "screens",
      "withStyle": true,
      "withTest": false,
      "withStory": false,
      "withLazy": false
    }
  }
}
